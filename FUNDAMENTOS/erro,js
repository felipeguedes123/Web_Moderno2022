throw {
    nome: erro.name,
    msg: erro.message,
    date: new Date
}



function imprimirNomeGritado(Obj) {
    try {
        console.log(obj.name.toUpperCase() + "!!!")
        } catch (e) {
            tratarErroELancar
        }  finally {
            console.log("final")
        }


}

const obj = { nome: "Roberto"}
imprimirNomeGritado(obj)
